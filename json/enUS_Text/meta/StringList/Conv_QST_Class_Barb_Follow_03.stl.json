 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "MenuLabel",
   "szText": "“What is in this cave?”",
   "hLabel": 4215274549
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line1.541E6",
   "szText": "A great and terrible beast. It has plagued the Steppes all season, laying waste to crops, caravans, and warriors alike.",
   "hLabel": 1509092124
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir1.541E6",
   "szText": "Only slighly somber. Such threats are common in the Steppes.",
   "hLabel": 3371979081
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line3.541E6",
   "szText": "Bring me its head as an offering to our ancestors. Then, we may call you Oxen Tribe.",
   "hLabel": 4092028062
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir3.541E6",
   "szText": "Direct. Reverent. Presenting the challenge.",
   "hLabel": 1659947723
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line7.541E6",
   "szText": "I will wait here for you to return... or until the sun rises again.",
   "hLabel": 667965346
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir7.541E6",
   "szText": "Direct. Not sugar coating.",
   "hLabel": 2530852303
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line9.BAE4.Bar",
   "szText": "You will not be waiting long.",
   "hLabel": 2471813870
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir9.BAE4.Bar",
   "szText": "Confident. Ready. Heading inside the cave.",
   "hLabel": 3385144827
  }
 ],
 "ptMapStringTable": "0"
}
