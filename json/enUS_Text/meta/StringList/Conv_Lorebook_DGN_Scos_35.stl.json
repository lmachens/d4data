 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "MenuLabel",
   "szText": "A tattered missive",
   "hLabel": 4215274549
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "DisembodiedVoice",
   "szText": "Tattered Missive",
   "hLabel": 155764431
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line1.B79B1",
   "szText": "In memory of Bishop Felweigh and her Knights. Someone must remember us.",
   "hLabel": 1562574988
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir1.B79B1",
   "szText": "Bishop FEL-way",
   "hLabel": 3425461945
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line3.B79B1",
   "szText": "We were ousted from Braestaig. We took hostages and retreated to a cave in the hills. But it’s all gone wrong.",
   "hLabel": 4145510926
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir3.B79B1",
   "szText": "Brey-stig",
   "hLabel": 1713430587
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line7.B79B1",
   "szText": "The prisoners spoke their heathen tongue and invoked the savage spirits of the wood. All was chaos. We were swarmed. Everyone, the glenfolk too. They died first, tied down. The Knights soon after. I’m ashamed to say I hid.",
   "hLabel": 721448210
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line5.B79B1",
   "szText": "Tell our story. Let it be known we died in sacrifice to the Light.",
   "hLabel": 2433479568
  }
 ],
 "ptMapStringTable": "0"
}
