 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Name",
   "szText": "The Traveling Scholar",
   "hLabel": 4062401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Toast",
   "szText": "Escort Scholar Aria to Túr Dúlra.",
   "hLabel": 141665387
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase6Description",
   "szText": "Aria, a travelling scholar, is excited about the druids returning to Túr Dúlra and wants to study with them.  She would like me to escort her through  the dangers of the Deep Forest and safely to Túr Dúlra.",
   "hLabel": 4210044747
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback36Header",
   "szText": "Take Aria to the ruins",
   "hLabel": 3742082015
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback86Header",
   "szText": "Search by the water",
   "hLabel": 2085932100
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback51Header",
   "szText": "Show artifact to Aria",
   "hLabel": 917249500
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase55Description",
   "szText": "After finding a druid relic in some old ruins, Aria wants to make a final stop at a waterfall where Vasily used to meditate.",
   "hLabel": 1662611391
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback54Header",
   "szText": "Take Aria to the waterfall overlook",
   "hLabel": 496686111
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase60Description",
   "szText": "We have been attacked by dangerous creatures while at Vasily's waterfall.  I must defeat them to protect Aria.",
   "hLabel": 3182929499
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback59Header",
   "szText": "Survive the ambush",
   "hLabel": 2659058660
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback67Header",
   "szText": "Speak to Aria",
   "hLabel": 4039860035
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase69Description",
   "szText": "We have passed through the most dangerous part of the Deep Forest and are on the threshold of Túr Dúlra.",
   "hLabel": 1984294596
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback71Header",
   "szText": "Speak to Aria",
   "hLabel": 254789534
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase76Description",
   "szText": "Before Aria goes to Túr Dúlra, she wants to find something of value to prove her worth.",
   "hLabel": 852156450
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback82Header",
   "szText": "Search for druid artifacts {DONE_OVER_NEEDED}",
   "hLabel": 1215027520
  }
 ],
