 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "MenuLabel",
   "szText": "The Offer",
   "hLabel": 4215274549
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line19.BD31C",
   "szText": "Hatred, Destruction, and Terror are forever; we will always need to fight them. But we can turn the tide of the Eternal Conflict in our favor. It starts here, with Mephisto's destruction--and you will lead the battles to come.",
   "hLabel": 3659028493
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line21.BD31C",
   "szText": "Simply take my hand. Fight for Sanctuary.",
   "hLabel": 1585989350
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line23.BD31C",
   "szText": "I will stay in the shadows.",
   "hLabel": 4168925288
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line25.BAE4",
   "szText": "I will never take your hand.",
   "hLabel": 3067911769
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir25.BAE4",
   "szText": "Determined--no way the player is going to side with Lilith.",
   "hLabel": 635831430
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line27.BD31C",
   "szText": "Then this is the future that awaits you.",
   "hLabel": 744862572
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir27.BD31C",
   "szText": "(disappointed, not enraged)",
   "hLabel": 2090590009
  }
 ],
 "ptMapStringTable": "0"
}
