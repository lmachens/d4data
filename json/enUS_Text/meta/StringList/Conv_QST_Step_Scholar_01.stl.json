 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "MenuLabel",
   "szText": "\"What are you doing out here?\"",
   "hLabel": 4215274549
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line1.1343B7",
   "szText": "I am Nurbolat, a scholar of history and religion. The Cathedral of Light seeks to heal Sanctuary's corruption. And I'm working to assist them.",
   "hLabel": 2397727211
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir1.1343B7",
   "szText": "Hopeful, excited about his work",
   "hLabel": 3743454648
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line3.1343B7",
   "szText": "This region was once overseen by the Hand of the Zakarum whom, I've gathered, were quite deft at rooting out corruption. I yearn to discover how exactly they guided those who strayed from the Light.",
   "hLabel": 1735267245
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir3.1343B7",
   "szText": "Informative, engaged",
   "hLabel": 3080994682
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line7.1343B7",
   "szText": "Thus far, my studies have led me here. I am currently following a thread to an abandoned marketplace nearby. Will you accompany me while I conduct my research?",
   "hLabel": 410347313
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir7.1343B7",
   "szText": "Excited, hopeful",
   "hLabel": 1756074750
  }
 ],
 "ptMapStringTable": "0"
}
