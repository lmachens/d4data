 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "MenuLabel",
   "szText": "\"That creature came for you. Care to explain?\"",
   "hLabel": 4215274549
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line1.157838",
   "szText": "His name is Varshan. He was my mentor. My priest, until the day Lilith came to our village.",
   "hLabel": 2400210759
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir1.157838",
   "szText": "To Player (5ft away.) Deep shame. Confessing.",
   "hLabel": 3745938196
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line3.157838",
   "szText": "Her words drove our people mad, turned them against us. I begged Varshan to run, but he refused to abandon them, so... I... I left him behind.",
   "hLabel": 1737750793
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir3.157838",
   "szText": "Grief. Deep shame.",
   "hLabel": 3083478230
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line5.157838",
   "szText": "In my guilt, I searched for a way to cure my people. Soon, I found Orbei... found the cages...",
   "hLabel": 1075290827
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir5.157838",
   "szText": "Deep shame. Hesitant to tell the next part.",
   "hLabel": 2421018264
  }
 ],
 "ptMapStringTable": "0"
}
