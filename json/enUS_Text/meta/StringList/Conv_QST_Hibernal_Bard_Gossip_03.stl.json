 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "MenuLabel",
   "szText": "\"What’s a bard doing here?\" ",
   "hLabel": 4215274549
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line1.171196",
   "szText": "I grew up in Hawezar with my mother, but it was here I was born. She told me stories of the season here.",
   "hLabel": 2402359552
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir1.171196",
   "szText": "Nostalgic as he thinks of his mother. \"hou-iz-ahr\"",
   "hLabel": 3748086989
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line3.171196",
   "szText": "They—ah, the Cathedral of Light, that is—They aren’t thrilled with a revival of the tradition. But I’m not alone—the people want a return to those roots.",
   "hLabel": 1739899586
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir3.171196",
   "szText": "A little chagrined.",
   "hLabel": 3085627023
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line5.171196",
   "szText": "The music, the dance... We can’t thrive in darkness without those. Who better to spread that then—well, me.",
   "hLabel": 1077439620
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir5.171196",
   "szText": "Starts off excited, then catches himself and turns shy.",
   "hLabel": 2423167057
  }
 ],
 "ptMapStringTable": "0"
}
