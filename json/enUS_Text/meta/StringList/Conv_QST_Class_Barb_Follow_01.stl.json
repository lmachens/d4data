{
 "__fileName__": "enUS_Text/meta/StringList/Conv_QST_Class_Barb_Follow_01.stl",
 "__snoID__": 1227113,
 "__type__": "StringListDefinition",
 "__typeHash__": 3672216444,
 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "MenuLabel",
   "szText": "\"Gerti sent me. I wish to join the Oxen Tribe.\"",
   "hLabel": 4215274549
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line1.541E6",
   "szText": "Do you now? The tales walk tall ahead of you, wanderer. Tell me, are you worthy of them?",
   "hLabel": 1509092124
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir1.541E6",
   "szText": "Arms folded. Smiling. Skeptical.",
   "hLabel": 3371979081
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line3.BAE4.Bar",
   "szText": "I would show you.",
   "hLabel": 2065005608
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir3.BAE4.Bar",
   "szText": "Confident. Ready.",
   "hLabel": 2978336565
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line5.541E6",
   "szText": "Good answer. To call yourself Oxen Tribe, you must earn it in blood, spilled in the sight of our ancestors. I will be their witness.",
   "hLabel": 2379996704
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir5.541E6",
   "szText": "Mild approval. Direct. Reverent.",
   "hLabel": 4242883661
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line7.541E6",
   "szText": "We make for the hunting grounds.",
   "hLabel": 667965346
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir7.541E6",
   "szText": "Direct. Presenting the way.",
   "hLabel": 2530852303
  }
 ],
 "ptMapStringTable": "0"
}
