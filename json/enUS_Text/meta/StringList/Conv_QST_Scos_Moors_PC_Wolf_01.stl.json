 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "MenuLabel",
   "szText": "\"Who might you be?\"",
   "hLabel": 4215274549
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line15.15481B",
   "szText": "(growling)",
   "hLabel": 4135412673
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir15.15481B",
   "szText": "Wolf is suspicious of this outsider--are they a Knight Penitent?",
   "hLabel": 1299777838
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line1.BAE4",
   "szText": "Easy, my friend. I’m not one of the Knights.",
   "hLabel": 3561462051
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir1.BAE4",
   "szText": "Cautious--interacting with a wild wolf. Don't know if they are a friend or enemy yet.",
   "hLabel": 494300592
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line17.15481B",
   "szText": "(sniffing)",
   "hLabel": 3472952707
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir17.15481B",
   "szText": "Wolf sniffs at the Player, trying to decide whether they are friend or foe.",
   "hLabel": 637317872
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line3.BAE4",
   "szText": "I’m looking for your master, Nafain. Think you could help me with that?",
   "hLabel": 3639732837
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir3.BAE4",
   "szText": "Hint of hopefulness--maybe this wolf can help us",
   "hLabel": 572571378
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line19.15481B",
   "szText": "(bark)",
   "hLabel": 2810492741
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir19.15481B",
   "szText": "A short affirmative growl-bark--not threatening, but the wolf's way of saying \"yes.\"",
   "hLabel": 4269825202
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line13.BAE4",
   "szText": "Good boy. Lead the way.",
   "hLabel": 1698173014
  }
 ],
 "ptMapStringTable": "0"
}
