 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Name",
   "szText": "Remnants of Faith",
   "hLabel": 4062401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Toast",
   "szText": "A young Crusader apprentice wishes to carry on her master's legacy.",
   "hLabel": 141665387
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase11Description",
   "szText": "Tomyris told me of a shortcut over a broken bridge. It should lead us straight to Maugan's Folly.",
   "hLabel": 1641010231
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback10Header",
   "szText": "Find the broken bridge",
   "hLabel": 950701463
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback13Header",
   "szText": "Cross the broken bridge",
   "hLabel": 530138074
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase17Description",
   "szText": "After crossing the broken bridge, we need to find the road. Taking it south will lead to Maugan's Folly.",
   "hLabel": 2273576061
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback106Header",
   "szText": "Enter Maugan's Folly",
   "hLabel": 1281528845
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase26Description",
   "szText": "The armor was pilfered by the deserters and criminals that now make this place their home. We must search Maugan's Works for the armor.",
   "hLabel": 2783948957
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback25Header",
   "szText": "Search Maugan's Work for Johanna's armor",
   "hLabel": 2781844029
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase32Description",
   "szText": "Donning her master's armor, Tomyris continues the name Johanna. Today, she is a true Crusader.",
   "hLabel": 830555290
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback31Header",
   "szText": "Speak with Johanna",
   "hLabel": 1579709466
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase23Description",
   "szText": "A desolate reminder of its former glory, we've reached Maugan's Folly. Tomyris' master should be nearby. Or rather, her corpse.",
   "hLabel": 320182394
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback22Header",
   "szText": "Find the crusader's armor",
   "hLabel": 3202407418
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase35Description",
   "szText": "Tomyris needs a guide through the Hawezar Wetlands. She wishes to return to Maugan's Folly to retrieve her late master's armor. I should meet her by the bridge leading into the Wetlands.",
   "hLabel": 3294321853
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback34Header",
   "szText": "Speak with Tomyris",
   "hLabel": 1159146077
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback44Header",
   "szText": "Check on Tomyris",
   "hLabel": 827916094
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback81Header",
   "szText": "Enter Maugan's Work",
   "hLabel": 4218526847
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase100Description",
   "szText": "At last! We've found the armor. I should speak with Tomyris.",
   "hLabel": 2972507974
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback99Header",
   "szText": "Speak with Tomyris",
   "hLabel": 1334138728
  }
 ],
 "ptMapStringTable": "0"
}
