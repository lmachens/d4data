 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Name",
   "szText": "以血还血",
   "hLabel": 4062401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Toast",
   "szText": "一个在绞刑架上的人恳求自由。",
   "hLabel": 141665387
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase0Description",
   "szText": "我在黑石警戒塔发现了一个强盗的俘虏。他求我从外面的强盗身上取回解开枷锁的钥匙。",
   "hLabel": 3577478917
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback51Header",
   "szText": "从强盗身上取得绞刑架的钥匙",
   "hLabel": 917249500
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase10Description",
   "szText": "我找到了尼萨尔枷锁的钥匙。我应该回到黑石警戒塔把他放出来。",
   "hLabel": 819754710
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback9Header",
   "szText": "解救绞刑架上的尼萨尔",
   "hLabel": 965040047
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase13Description",
   "szText": "我救了尼萨尔，看来在绞刑架上的日子让他很难受。他求我给他一瓶能治疗伤势的药水。",
   "hLabel": 3283521273
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback12Header",
   "szText": "把你的治疗药水给尼萨尔一瓶",
   "hLabel": 3533637401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase16Description",
   "szText": "尼萨尔和我正在前往柏尔札的藏身处，直面囚禁他的罪魁祸首。",
   "hLabel": 1452320540
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback15Header",
   "szText": "进入柏尔札的藏身处",
   "hLabel": 3113074012
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase19Description",
   "szText": "我把尼萨尔救了出来，看来在绞刑架里的日子让他很难受。",
   "hLabel": 3916087103
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback18Header",
   "szText": "与尼萨尔交谈",
   "hLabel": 2692510623
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase22Description",
   "szText": "尼萨尔想要报复柏尔札——那个把他送上绞架的人。但首先，他需要一把武器才能战斗。",
   "hLabel": 3793894169
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback72Header",
   "szText": "帮助尼萨尔选择一把武器",
   "hLabel": 1546257503
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase62Description",
   "szText": "我在黑石警戒塔发现了一个强盗的俘虏。他求我从外面的强盗身上取回解开枷锁的钥匙。",
   "hLabel": 530473245
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback64Header",
   "szText": "找到破败营地",
   "hLabel": 165456128
  }
 ],
 "ptMapStringTable": "0"
}
