 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "MenuLabel",
   "szText": "一封破烂的信函",
   "hLabel": 4215274549
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "DisembodiedVoice",
   "szText": "破烂的信函",
   "hLabel": 155764431
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line1.B79B1",
   "szText": "谨此纪念费尔维主教及其骑士团。必须有人铭记我们。",
   "hLabel": 1562574988
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line3.B79B1",
   "szText": "被赶出布雷斯塔后，我们劫持了人质，退到一个山洞里。但事态急转直下。",
   "hLabel": 4145510926
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line5.B79B1",
   "szText": "讲述我们的故事吧。让大家知道，我们已为圣光献生。",
   "hLabel": 2433479568
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line7.B79B1",
   "szText": "人质用异族语言祈求着林间的野蛮神灵。所有人乱作一团，那些索格伦人也不例外。他们被绑着先死了，骑士团很快也没了。我很羞愧我藏了起来。",
   "hLabel": 721448210
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line1.B79B1.F",
   "szText": "谨此纪念费尔维主教及其骑士团。必须有人铭记我们。",
   "hLabel": 837114336
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line3.B79B1.F",
   "szText": "被赶出布雷斯塔后，我们劫持了人质，退到一个山洞里。但事态急转直下。",
