 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Name",
   "szText": "残余的信仰",
   "hLabel": 4062401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Toast",
   "szText": "一位年轻的圣教军学徒希望继承她师傅的遗产。",
   "hLabel": 141665387
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase11Description",
   "szText": "托米蕊斯告诉我在断桥上有一条捷径。我们可以直接从那里抵达莫甘堡。",
   "hLabel": 1641010231
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback10Header",
   "szText": "找到断桥",
   "hLabel": 950701463
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback13Header",
   "szText": "穿过断桥",
   "hLabel": 530138074
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase17Description",
   "szText": "过了断桥以后，我们得找到出路。往南走就是莫甘堡了。",
   "hLabel": 2273576061
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback106Header",
   "szText": "进入莫甘堡",
   "hLabel": 1281528845
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase26Description",
   "szText": "护甲被如今在这里安家的逃兵和罪犯偷走了。我们必须在莫甘的工坊中寻找护甲。",
   "hLabel": 2783948957
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback25Header",
   "szText": "在莫甘的工坊寻找乔汉娜的护甲",
   "hLabel": 2781844029
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase32Description",
   "szText": "穿上她师傅的盔甲，托米蕊斯延续了乔汉娜之名。今天，她是一个真正的圣教军。",
   "hLabel": 830555290
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback31Header",
   "szText": "与乔汉娜交谈",
   "hLabel": 1579709466
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase23Description",
   "szText": "我们抵达了莫甘堡，这里昔日的辉煌不再，只剩下一片荒芜。我们应该能在附近找到托近米蕊斯的导师——或者她的遗骸。",
   "hLabel": 320182394
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback22Header",
   "szText": "找到圣教军的护甲",
   "hLabel": 3202407418
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase35Description",
   "szText": "托米蕊斯需要一名向导帮忙穿越哈维泽湿地。她希望回到莫甘堡取回她已故导师的护甲。我应该在通往湿地的桥上与她会面。",
   "hLabel": 3294321853
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback34Header",
   "szText": "与托米蕊斯交谈",
   "hLabel": 1159146077
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback44Header",
   "szText": "查看托米蕊斯的情况",
   "hLabel": 827916094
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback81Header",
   "szText": "进入莫甘的工坊",
   "hLabel": 4218526847
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase100Description",
   "szText": "我们终于找到了护甲。我应该和托米蕊斯谈谈。",
   "hLabel": 2972507974
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback99Header",
   "szText": "与托米蕊斯交谈",
   "hLabel": 1334138728
  }
 ],
 "ptMapStringTable": "0"
}
