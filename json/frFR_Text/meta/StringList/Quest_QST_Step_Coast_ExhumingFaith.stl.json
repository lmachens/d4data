 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Name",
   "szText": "Exhumation de la foi",
   "hLabel": 4062401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Toast",
   "szText": "Suivez le sentier de pèlerinage du Zakarum pour obtenir de fabuleuses récompenses.",
   "hLabel": 141665387
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback9Header",
   "szText": "Trouvez le repère du Zakarum au canal de Khotun.",
   "hLabel": 965040047
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback12Header",
   "szText": "Trouvez le repère du Zakarum sur le surplomb.",
   "hLabel": 3533637401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase10Description",
   "szText": "J’ai trouvé le journal d’un moine mort, ainsi qu’un message dans lequel figurent d’énigmatiques indices menant à un parchemin d’une grande puissance caché sur les côtes du canal de Khotun. Je devrais chercher le premier endroit mentionné dans le message.",
   "hLabel": 819754710
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase13Description",
   "szText": "J’ai trouvé la grande épave qui figurait dans le message. Maintenant, je dois trouver un repère quelque part sur un surplomb.",
   "hLabel": 3283521273
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase19Description",
   "szText": "Au bout de la piste se trouve un étrange parchemin. Ce doit être celui dont le moine parlait.",
   "hLabel": 3916087103
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback35Header",
   "szText": "Lisez le parchemin étrange.",
   "hLabel": 2450614046
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase27Description",
   "szText": "Le fait de lire ce parchemin invoque instantanément un démon. Il s’agit probablement de la ruse utilisée par le démon pour entrer dans Sanctuaire.",
   "hLabel": 3605204478
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback26Header",
   "szText": "Tuez le démon.",
   "hLabel": 4073311998
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback49Header",
   "szText": "Lisez ce qui est inscrit sur le premier repère.",
   "hLabel": 2990288643
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback52Header",
   "szText": "Lisez ce qui est inscrit sur le second repère.",
   "hLabel": 2208717469
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase53Description",
   "szText": "J’ai trouvé l’arbre isolé. Le parchemin du moine ne doit pas être loin.",
   "hLabel": 20100349
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback56Header",
   "szText": "Trouvez l’emplacement final.",
   "hLabel": 3079622049
  }
 ],
 "ptMapStringTable": "0"
}
