 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Name",
   "szText": "Esca infida",
   "hLabel": 4062401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Toast",
   "szText": "Lyndon ha un piano infallibile per scovare la talpa della Palude Sperduta.",
   "hLabel": 141665387
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase0Description",
   "szText": "Il primo luogo è stato un fallimento, ma Lyndon è convinto che il secondo posto sarà quello vincente. Rodney gli ha dato diverse grane di recente.",
   "hLabel": 3577478917
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback5Header",
   "szText": "Raggiungi il luogo della prossima imboscata",
   "hLabel": 94135467
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase17Description",
   "szText": "Lyndon ha organizzato tre imboscate per attirare la talpa. In base a dove la subiremo, sapremo chi è la talpa. La prima posizione è nelle vicinanze.",
   "hLabel": 2273576061
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback16Header",
   "szText": "Raggiungi il luogo della prima imboscata",
   "hLabel": 109574685
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase23Description",
   "szText": "Lyndon crede che non possa esserci una talpa e che l'imboscata sia stata solo una coincidenza. L'ultima posizione è stata riferita a Orton.",
   "hLabel": 320182394
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback22Header",
   "szText": "Raggiungi il luogo dell'ultima imboscata",
   "hLabel": 3202407418
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase29Description",
   "szText": "Per ora è tutto tranquillo. Forse Lyndon è solo paranoico e l'attacco è stato una semplice coincidenza.",
   "hLabel": 952748224
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback28Header",
   "szText": "Entra nella Baracca Isolata",
   "hLabel": 2361280640
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase74Description",
   "szText": "Eccoci alla prima posizione. A parte un'infestazione di annegati, sembra tutto tranquillo.",
   "hLabel": 3504612704
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback73Header",
   "szText": "Entra nella Radura Appartata",
   "hLabel": 2837725472
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase81Description",
   "szText": "Lyndon ha un altro incarico per me. Sicuramente sarà una follia per trovare questa presunta \"talpa\". Dobbiamo parlare lontano da orecchie indiscrete.",
   "hLabel": 2372474558
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback80Header",
   "szText": "Parla con Lyndon fuori dalla Palude Sperduta",
   "hLabel": 2927058878
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback86Header",
   "szText": "Parla con Lyndon",
   "hLabel": 2085932100
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase125Description",
   "szText": "Il primo luogo è stato un fallimento, ma Lyndon è convinto che il secondo posto sarà quello vincente. Rodney gli ha dato diverse grane di recente.",
   "hLabel": 1152107821
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback124Header",
   "szText": "Entra nella Radura Tranquilla",
   "hLabel": 2331100237
  }
 ],
