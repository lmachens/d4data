  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase10Description",
   "szText": "Ho recuperato la chiave per liberare Nizaar. Devo tornare al Torrione d'Onice per liberarlo.",
   "hLabel": 819754710
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback9Header",
