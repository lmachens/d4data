 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Name",
   "szText": "Resti della fede",
   "hLabel": 4062401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Toast",
   "szText": "Una giovane apprendista Crociata desidera portare avanti il retaggio della sua maestra.",
   "hLabel": 141665387
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase11Description",
   "szText": "Tomyris mi ha indicato una scorciatoia, oltrepassando un ponte rotto. Dovrebbe condurci direttamente alla Follia di Maugan.",
   "hLabel": 1641010231
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback10Header",
   "szText": "Trova il ponte rotto",
   "hLabel": 950701463
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback13Header",
   "szText": "Attraversa il ponte rotto",
   "hLabel": 530138074
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase17Description",
   "szText": "Dopo aver superato il ponte rotto, dovremo trovare la strada. Andare a sud ci condurrà alla Follia di Maugan.",
   "hLabel": 2273576061
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback106Header",
   "szText": "Entra nella Follia di Maugan",
   "hLabel": 1281528845
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase26Description",
   "szText": "L'armatura è stata rubata dai disertori e criminali che hanno fatto di questo luogo la propria dimora. Dobbiamo cercare l'armatura tra le Opere di Maugan.",
   "hLabel": 2783948957
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback25Header",
   "szText": "Cerca l'armatura di Johanna tra le Opere di Maugan",
   "hLabel": 2781844029
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase32Description",
   "szText": "Indossando l'armatura della sua maestra, Tomyris porterà avanti il nome di Johanna. Oggi è diventata una vera Crociata.",
   "hLabel": 830555290
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback31Header",
   "szText": "Parla con Johanna",
   "hLabel": 1579709466
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase23Description",
   "szText": "Abbiamo raggiunto la Follia di Maugan, desolante promemoria della sua vecchia gloria. La maestra di Tomyris dovrebbe trovarsi nelle vicinanze. O meglio, il suo cadavere.",
   "hLabel": 320182394
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback22Header",
   "szText": "Trova l'armatura della Crociata",
   "hLabel": 3202407418
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase35Description",
   "szText": "Tomyris ha bisogno di una guida per attraversare le paludi dell'Hawezar. Desidera fare ritorno alla Follia di Maugan per recuperare l'armatura della sua fu maestra. Dovrei incontrarla al ponte che conduce alle paludi.",
   "hLabel": 3294321853
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback34Header",
   "szText": "Parla con Tomyris",
   "hLabel": 1159146077
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback44Header",
   "szText": "Verifica come sta Tomyris",
   "hLabel": 827916094
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback81Header",
   "szText": "Entra nelle Opere di Maugan",
   "hLabel": 4218526847
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase100Description",
   "szText": "Finalmente abbiamo trovato l'armatura. Dovrei parlare con Tomyris.",
   "hLabel": 2972507974
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback99Header",
   "szText": "Parla con Tomyris",
   "hLabel": 1334138728
  }
 ],
