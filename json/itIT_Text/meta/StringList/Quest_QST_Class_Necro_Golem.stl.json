 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Name",
   "szText": "Negromante: Richiamo dell'Oltretomba",
   "hLabel": 4062401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase8Description",
   "szText": "Maltorius il Negromante mi insegnerà come creare un Golem. Devo procurarmi delle ossa che costituiranno il suo corpo.",
   "hLabel": 1557588493
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback7Header",
   "szText": "Ottieni {ITEM}: {DONE_OVER_NEEDED}",
   "hLabel": 2677071405
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase11Description",
   "szText": "Maltorius il Negromante mi insegnerà come creare un Golem. Devo raccogliere delle anime erranti per donare al Golem una mente e una propria volontà. ",
   "hLabel": 1641010231
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback10Header",
   "szText": "Entra nella Caverna Amara",
   "hLabel": 950701463
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase22Description",
   "szText": "Riesco a sentire nella mia mente le voci echeggianti dei morti. Mi hanno chiesto di onorare la mia vocazione negromantica con un pellegrinaggio al Santuario di Rathma.",
   "hLabel": 3793894169
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback21Header",
   "szText": "Prega al Santuario di Rathma",
   "hLabel": 1910939449
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase25Description",
   "szText": "Al Santuario di Rathma è apparsa una strana visione, un essere intriso di energie negromantiche. Sembra ansioso di parlare con me. ",
   "hLabel": 1962693436
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback24Header",
   "szText": "Parla con la presenza spettrale",
   "hLabel": 1490376060
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase31Description",
   "szText": "Maltorius il Negromante mi insegnerà come creare un Golem. Devo trovare la caverna che custodisce la Giara delle Anime, per imprigionare le anime destinate alla creazione del Golem. ",
   "hLabel": 9299769
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback30Header",
   "szText": "Trova la caverna che custodisce il manufatto vile",
   "hLabel": 288241497
  }
 ],
