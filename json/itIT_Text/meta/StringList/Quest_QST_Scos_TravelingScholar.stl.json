 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Name",
   "szText": "La studiosa itinerante",
   "hLabel": 4062401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Toast",
   "szText": "Scorta la Studiosa Aria a Túr Dúlra.",
   "hLabel": 141665387
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase6Description",
   "szText": "Aria, una studiosa viaggiatrice, è entusiasta del ritorno dei Druidi a Túr Dúlra e vuole studiare con loro. Vorrebbe che la scortassi attraverso i pericoli della Selva Nera e fino a Túr Dúlra.",
   "hLabel": 4210044747
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback36Header",
   "szText": "Accompagna Aria alle rovine",
   "hLabel": 3742082015
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback86Header",
   "szText": "Cerca vicino all'acqua",
   "hLabel": 2085932100
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback51Header",
   "szText": "Mostra il manufatto ad Aria",
   "hLabel": 917249500
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase55Description",
   "szText": "Dopo aver trovato una reliquia druidica in alcune vecchie rovine, Aria vuole fare un'ultima tappa presso una cascata dove Vasily restava a meditare.",
   "hLabel": 1662611391
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback54Header",
   "szText": "Porta Aria al promontorio della cascata",
   "hLabel": 496686111
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase60Description",
   "szText": "Alla cascata di Vasily abbiamo subito un attacco da delle creature pericolose. Devo sconfiggerle per proteggere Aria.",
   "hLabel": 3182929499
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback59Header",
   "szText": "Sopravvivi all'imboscata",
   "hLabel": 2659058660
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback67Header",
   "szText": "Parla con Aria",
   "hLabel": 4039860035
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase69Description",
   "szText": "Abbiamo superato la parte più pericolosa della Selva Nera e siamo al confine di Túr Dúlra.",
   "hLabel": 1984294596
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback71Header",
   "szText": "Parla con Aria",
   "hLabel": 254789534
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase76Description",
   "szText": "Prima che Aria vada a Túr Dúlra, vuole trovare qualcosa di prezioso per dimostrare il suo valore.",
   "hLabel": 852156450
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback82Header",
   "szText": "Cerca i manufatti druidici {DONE_OVER_NEEDED}",
   "hLabel": 1215027520
  }
 ],
 "ptMapStringTable": "0"
}
