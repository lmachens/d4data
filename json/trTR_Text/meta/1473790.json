 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Name",
   "szText": "Korku ve İnanç Savaşı",
   "hLabel": 4062401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase0Description",
   "szText": "Kafatası, Erys'in aceleyle Işık Katedrali'ne doğru yola koyulduğunu söyledi. Başından beri lanetimden korkuyordu... Yine de onu bulmalı ve neler olduğunu öğrenmeliyim.",
   "hLabel": 3577478917
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback2Header",
   "szText": "Işık Katedrali'nde Erys'i ara",
   "hLabel": 514698856
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase5Description",
   "szText": "Mühürlü kapının durumunu kontrol etmek için döndüğümde beni bir saldırının beklediğini ve Erys'in ortalıkta olmadığını gördüm. Neler olduğunu öğrenmem gerek.",
   "hLabel": 3388789226
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback10Header",
   "szText": "Erys ile konuş",
   "hLabel": 950701463
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase25Description",
   "szText": "Kan Hasadı'nı durdurduk ve kurtarabildiğimiz kadar esiri kurtardık. Erys ile hemen tekrar buluşmalıyım. Lort Zir'in yenilgiden kurtulup iyileşmesine izin veremeyiz.",
   "hLabel": 1962693436
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback24Header",
   "szText": "Ked Bardu'daki Erys'in yanına dön",
   "hLabel": 1490376060
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase81Description",
   "szText": "Erys'i Katedral'in dışında, çaresiz bir kalabalığın ortasında buldum. Bir şeyler çok yanlış gidiyor. Erys'le konuşup ne olduğunu öğrenmeliyim.",
   "hLabel": 2372474558
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback80Header",
   "szText": "Erys ile konuş",
   "hLabel": 2927058878
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase90Description",
   "szText": "Lort Zir'in ordusu sayısız masumun kanını akıtıyor ve Katedral müdahale etmiyor. Erys ile birbirimize güvenip düşmanla kendi başımıza savaşmaktan başka çaremiz yok.",
   "hLabel": 2882847454
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback89Header",
   "szText": "Kan Hasadını ara ve durdur",
   "hLabel": 1665368711
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback98Header",
   "szText": "Sezon Yolculuğunun 3. Bölümünü tamamla",
   "hLabel": 42670759
  }
 ],
