 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Name",
   "szText": "Bitwa Strachu i Wiary",
   "hLabel": 4062401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase0Description",
   "szText": "Według czaszki Erys pospiesznie wyruszyła do Katedry Światłości. Od początku bała się mojej klątwy... Mimo to muszę ją odszukać i dowiedzieć się, co się wydarzyło.",
   "hLabel": 3577478917
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback2Header",
   "szText": "Poszukaj Erys w Katedrze Światłości",
   "hLabel": 514698856
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase5Description",
   "szText": "Gdy |5wróciłem:wróciłam;, by sprawdzić postępy w otwieraniu zapieczętowanych wrót, na miejscu czekała na mnie zasadzka, a po Erys nie było śladu. Muszę się dowiedzieć, co się stało.",
   "hLabel": 3388789226
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback10Header",
   "szText": "Porozmawiaj z Erys",
   "hLabel": 950701463
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase25Description",
   "szText": "Udało nam się powstrzymać Krwawe Żniwa i ocalić pozostałych porwanych. Muszę się natychmiast przegrupować z Erys. Nie możemy pozwolić Władcy Zirowi podnieść się po tej porażce.",
   "hLabel": 1962693436
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback24Header",
   "szText": "Wróć do Erys w Ked Bardu",
   "hLabel": 1490376060
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase81Description",
   "szText": "Zdesperowany tłum otoczył Erys obok Katedry. Coś jest nie tak. Muszę pomówić z Erys i dowiedzieć się, o co chodzi.",
   "hLabel": 2372474558
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback80Header",
   "szText": "Porozmawiaj z Erys",
   "hLabel": 2927058878
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase90Description",
   "szText": "Armia Władcy Zira wykrwawia niezliczone niewinne istoty, a Katedra nie ma zamiaru interweniować. Erys i ja nie mamy wyboru. Musimy sobie zaufać i samodzielnie z nimi walczyć.",
   "hLabel": 2882847454
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback89Header",
   "szText": "Odszukaj Krwawe Żniwa i je powstrzymaj",
   "hLabel": 1665368711
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback98Header",
   "szText": "Ukończ Rozdział 3 postępów sezonowych",
   "hLabel": 42670759
  }
 ],
