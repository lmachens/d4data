 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Name",
   "szText": "他所知的世界",
   "hLabel": 4062401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase113Description",
   "szText": "墨法斯是恐懼之王迪亞布羅的僕從。為了確保驅魔發揮作用，我需要運用這符石，以迪亞布羅的權威壓倒墨法斯。",
   "hLabel": 2472935658
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback112Header",
   "szText": "將尚無效力的符石調和為恐懼",
   "hLabel": 79394282
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase116Description",
   "szText": "我需要獲得迪亞布羅的祝福才能封印符石。我先前就在希沃山做過相同的事，希望我不會再次受到詛咒。",
   "hLabel": 641734925
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback115Header",
   "szText": "收下迪亞布羅的祝福",
   "hLabel": 3953798189
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback118Header",
   "szText": "使用費斯傑利祭壇封印地獄祝福符石",
   "hLabel": 3533234800
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase122Description",
   "szText": "我找到了圖書館。這裡頭肯定有本書能告訴我們該如何驅逐墨法斯。",
   "hLabel": 2983308554
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback151Header",
   "szText": "研究費斯傑利的儀式",
   "hLabel": 1757973677
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback143Header",
   "szText": "在卡爾蒂姆圖書館尋找費斯傑利的著作",
   "hLabel": 377172302
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase161Description",
   "szText": "符石已被迪亞布羅祝福。現在得找到費斯傑利的據點並將魔法封印在裡頭。",
   "hLabel": 3193599405
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback160Header",
   "szText": "找到費斯傑利的據點",
   "hLabel": 135275725
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback186Header",
   "szText": "研究費斯傑利的儀式",
   "hLabel": 2926656277
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase190Description",
   "szText": "阿育贊對惡魔所知不多。他要我前往卡爾蒂姆，找到費斯傑利留下的文獻。他恐怕不清楚卡爾蒂姆的現狀。",
   "hLabel": 2072261839
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback199Header",
   "szText": "達到等級 20 以解鎖珠寶匠",
   "hLabel": 2174862905
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback189Header",
   "szText": "在珍品商處製作一個無常機奴石堆",
   "hLabel": 2506092888
  }
 ],
 "ptMapStringTable": "0"
}
