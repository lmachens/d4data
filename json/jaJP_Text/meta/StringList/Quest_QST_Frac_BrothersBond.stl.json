 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Name",
   "szText": "血の結束",
   "hLabel": 4062401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Toast",
   "szText": "信心深い青年が、手に負えない弟のことで助けを求めている。",
   "hLabel": 141665387
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase22Description",
   "szText": "マーグレイヴの村人ラクサンが兄弟を探している。商人たちが盗賊に襲われた件に巻き込まれていたらしい。最後に目撃された場所に向かう前に、ペリサンが戻ってきていないか町で聞き込みをしてみよう。",
   "hLabel": 3793894169
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback21Header",
   "szText": "マトヴェイ司祭と話す",
   "hLabel": 1910939449
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback50Header",
   "szText": "メリダンと話す",
   "hLabel": 3920748827
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback24Header",
   "szText": "衛兵クヴェトコと話す",
   "hLabel": 1490376060
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase100Description",
   "szText": "ペリサンを見つけ答えを得るため、ラクサンと共に盗賊の隠れ家に入り込んだ。",
   "hLabel": 2972507974
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback99Header",
   "szText": "刺客の隠場に入り、ペリサンを探す",
   "hLabel": 1334138728
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase122Description",
   "szText": "ラクサンは、死んだ盗賊に見覚えがあった。ペリサンの知り合いらしい。盗賊どもの隠れ家がどこにあるか知っているので、案内してくれるそうだ。",
   "hLabel": 2983308554
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback121Header",
   "szText": "ラクサンが咎人の猟場にある盗賊の隠れ家を探すのを手伝う",
   "hLabel": 2751663626
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase127Description",
   "szText": "商人たちが野宿したらしき跡を見つけた。ペリサンがいた形跡がないか、何が起きたのかを示す手がかりを探そう。",
   "hLabel": 2794618863
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback126Header",
   "szText": "手がかりを求めて辺りを探索する",
   "hLabel": 619068879
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase89Description",
   "szText": "ペリサンが商人の一隊と出発して以来、マーグレイヴの誰も彼の姿を見ていない。ラクサンと共に隊商の野営地を調べなくては。",
   "hLabel": 352584134
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback88Header",
   "szText": "ラクサンを弟が消息を絶った場所に連れて行く",
   "hLabel": 373900742
  }
 ],
 "ptMapStringTable": "0"
}
