 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Name",
   "szText": "かつての世界",
   "hLabel": 4062401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase113Description",
   "szText": "マルファスは恐怖の帝王ディアブロのしもべだった。悪魔祓いの儀式を成功させるには、このルーンストーンを通してマルファスにディアブロの権威を思い知らせねば。",
   "hLabel": 2472935658
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback112Header",
   "szText": "不活性のルーンストーンを恐怖に調律する",
   "hLabel": 79394282
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase116Description",
   "szText": "ルーンストーンを封印するにはディアブロの祝福を受ける必要がある。以前にもシヴォ山でやったことがあるが、また呪われないよう祈るしかない。",
   "hLabel": 641734925
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback115Header",
   "szText": "ディアブロの祝福を受ける",
   "hLabel": 3953798189
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback118Header",
   "szText": "ヴィジェレイの祭壇を使って地獄の祝福のルーンストーンを封印する",
   "hLabel": 3533234800
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase122Description",
   "szText": "書庫を見つけた。ここにある本の1冊にマルファスを祓うための知識が記されているだろう。",
   "hLabel": 2983308554
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback151Header",
   "szText": "ヴィジェレイの儀式について調べる",
   "hLabel": 1757973677
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback143Header",
   "szText": "カルディウムの図書室でヴィジェレイの記録を見つける",
   "hLabel": 377172302
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase161Description",
   "szText": "ルーンストーンにはディアブロの祝福がかけられている。ヴィジェレイの拠点を見つけて石の中の魔法を封じなければならない。",
   "hLabel": 3193599405
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback160Header",
   "szText": "ヴィジェレイの拠点を見つける",
   "hLabel": 135275725
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback186Header",
   "szText": "ヴィジェレイの儀式について調べる",
   "hLabel": 2926656277
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase190Description",
   "szText": "アユザンは悪魔には詳しくないため、カルディウムへ行ってヴィジェレイの文書を探すよう頼まれた。彼がカルディウムの現状を知っているとは思えないが。",
   "hLabel": 2072261839
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback199Header",
   "szText": "レベル20に到達して宝石職人を解放する",
   "hLabel": 2174862905
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback189Header",
   "szText": "貴重品管理係のもとで不明な臣下石の箱を作成する",
   "hLabel": 2506092888
  }
 ],
 "ptMapStringTable": "0"
}
