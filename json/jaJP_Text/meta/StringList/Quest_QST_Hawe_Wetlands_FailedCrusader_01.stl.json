 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Name",
   "szText": "信仰の残骸",
   "hLabel": 4062401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Toast",
   "szText": "若きクルセイダー見習いが、師匠の遺志を継ぎたいと願っている。",
   "hLabel": 141665387
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase11Description",
   "szText": "トミリスから、壊れた橋を渡る近道について聞いた。これで、モーガンの要塞にまっすぐたどり着くはずだ。",
   "hLabel": 1641010231
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback10Header",
   "szText": "壊れた橋を探す",
   "hLabel": 950701463
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback13Header",
   "szText": "壊れた橋を渡る",
   "hLabel": 530138074
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase17Description",
   "szText": "壊れた橋を渡ったら、道を探さなくてはならない。その道を南に進めば、モーガンの要塞にたどりつくはずだ。",
   "hLabel": 2273576061
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback106Header",
   "szText": "モーガンの要塞に入る",
   "hLabel": 1281528845
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase26Description",
   "szText": "師匠の鎧は、脱走兵と犯罪者に盗まれてしまった。奴らはこの場所を住処としてしまっている。モーガンの成果で鎧を探さなくては。",
   "hLabel": 2783948957
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback25Header",
   "szText": "モーガンの成果でヨハンナの鎧を探す",
   "hLabel": 2781844029
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase32Description",
   "szText": "師匠の鎧を身に纏い、トミリスはヨハンナの名を受け継ぐ。今日、彼女は真のクルセイダーとなる。",
   "hLabel": 830555290
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback31Header",
   "szText": "ヨハンナと話す",
   "hLabel": 1579709466
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase23Description",
   "szText": "かつての栄華が偲ばれる侘しい場所、モーガンの要塞に到着した。きっとこの近くに、トミリスの師匠が…いや、師匠の死体があるはずだ。",
   "hLabel": 320182394
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback22Header",
   "szText": "クルセイダーの鎧を探す",
   "hLabel": 3202407418
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase35Description",
   "szText": "ハウザーの湿地帯を通り抜けるために、トミリスが案内役を必要としている。モーガンの要塞に戻って、亡き師匠の鎧を回収したいのだ。湿地へと続く橋の横で彼女と落ち合おう。",
   "hLabel": 3294321853
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback34Header",
   "szText": "トミリスと話す",
   "hLabel": 1159146077
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback44Header",
   "szText": "トミリスの様子を見る",
   "hLabel": 827916094
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback81Header",
   "szText": "モーガンの成果に入る",
   "hLabel": 4218526847
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase100Description",
   "szText": "ついに、鎧を見つけた。トミリスと話さなくては。",
   "hLabel": 2972507974
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback99Header",
   "szText": "トミリスと話す",
   "hLabel": 1334138728
  }
 ],
 "ptMapStringTable": "0"
}
