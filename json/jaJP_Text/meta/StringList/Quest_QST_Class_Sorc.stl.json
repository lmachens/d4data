 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Name",
   "szText": "ソーサラー: 魔術師の遺産",
   "hLabel": 4062401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase0Description",
   "szText": "幻の風に乗って、山の中にいる何かの声がかすかに聞こえる。こちらに呼びかけているのか？調べてみなくては。",
   "hLabel": 3577478917
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback2Header",
   "szText": "幽界の声についていく",
   "hLabel": 514698856
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase5Description",
   "szText": "直感が、ずっと頭に響いていた声はこの男のものだと告げているが、それが誰なのかはわからない。",
   "hLabel": 3388789226
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback4Header",
   "szText": "見知らぬ者と話す",
   "hLabel": 3097634794
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase14Description",
   "szText": "忘れられた古文書を手に入れたので、モーダリンの所に戻らなくてはならない。彼なら解読を助けてくれるだろう。",
   "hLabel": 4104776794
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback13Header",
   "szText": "モーダリンのもとに戻る",
   "hLabel": 530138074
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase17Description",
   "szText": "モーダリンから古文書について詳しく知るための儀式に招かれた。",
   "hLabel": 2273576061
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback16Header",
   "szText": "放棄された小屋に入る",
   "hLabel": 109574685
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase45Description",
   "szText": "モーダリンは怪我をしているかもしれない。様子を確認しなくては。",
   "hLabel": 330982974
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback44Header",
   "szText": "モーダリンと話す",
   "hLabel": 827916094
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback49Header",
   "szText": "放棄された小屋に入る",
   "hLabel": 2990288643
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase53Description",
   "szText": "学者のモーダリンから、失われた書庫の中にある忘れられた古文書について聞いた。古文書を見つけ、そこに書かれた秘密を解き明かす必要がある。",
   "hLabel": 20100349
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback52Header",
   "szText": "{ITEM}を回収する",
