 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Name",
   "szText": "不遜なる者、微笑み返す",
   "hLabel": 4062401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Toast",
   "szText": "破顔の者の贈り物は、必ず微笑みを誘う。",
   "hLabel": 141665387
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase15Description",
   "szText": "骨粉がオガイに見せたのは、悪魔が蠢く暗黒の迷宮だ。破顔の者の祠の地下に広がっているという。迷宮の内部にある古代の供物を回収するよう、破顔の者から求められたそうだ。自分の力を証明しようと必死のオガイは、協力してほしいと頼んでいる。",
   "hLabel": 631065019
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback17Header",
   "szText": "破顔の迷宮の中のフォールンから破滅の奉納品を回収する ({DONE}/{NEEDED})",
   "hLabel": 1401042654
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase18Description",
   "szText": "破顔の者に捧げられた古代の供物を、破顔の迷宮から回収してきた。オガイの所に持って行き、彼が見たという幻が本当のことだったのか確かめよう。",
   "hLabel": 3094831582
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback20Header",
   "szText": "オガイの所に戻る",
   "hLabel": 619471480
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase21Description",
   "szText": "オガイから、破顔の者の供物を彼の周囲に並べるよう言われた。古代の存在に注意を引くためだ。\r\n\r\nようやくオガイが破顔の者との謁見を実現できるのか、これでわかるだろう。",
   "hLabel": 2972638648
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback23Header",
   "szText": "儀式の石に捧げものを置く ({DONE}/{NEEDED})",
   "hLabel": 198908091
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase24Description",
   "szText": "オガイから、破顔の者の供物を彼の周囲に並べるよう言われた。古代の存在に注意を引くためだ。\r\n\r\nようやくオガイが破顔の者との謁見を実現できるのか、これでわかるだろう。",
   "hLabel": 1141437915
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback26Header",
   "szText": "オガイと話す",
   "hLabel": 4073311998
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase39Description",
   "szText": "破顔の者の注意を引き、奴の「永遠の贈り物」を手に入れるため、為すべきことは何か。自分にはわかっているとオガイはそう確信している。オガイに頼まれた通り、北岸で彼と会って計画を進めなくては。",
   "hLabel": 2284376641
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback41Header",
   "szText": "破顔の者の近くでオガイと話す",
   "hLabel": 1248479483
  }
 ],
 "ptMapStringTable": "0"
}
