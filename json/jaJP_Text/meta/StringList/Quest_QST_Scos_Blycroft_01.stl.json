 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Name",
   "szText": "破滅の荒野",
   "hLabel": 4062401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Toast",
   "szText": "農夫のフィーランに協力して、ブライクロフトをカズラから守る。",
   "hLabel": 141665387
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase8Description",
   "szText": "草地丘陵のブライクロフトにある農場は、絶え間ないカズラの襲撃に悩まされている。\r\n\r\nこの地に新しくやってきた若きドルイドのアルタイルが、奴らの攻撃を防ぐための結界石を持ってきている。石を農場に設置するのを手伝おう。",
   "hLabel": 1557588493
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback26Header",
   "szText": "ドルイドの結界石を拾う",
   "hLabel": 4073311998
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback34Header",
   "szText": "結界石を置く: {DONE_OVER_NEEDED}",
   "hLabel": 1159146077
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase49Description",
   "szText": "結界石の設置が終わった。フィーランの所に戻って、まだ何かやることがないか聞こう。",
   "hLabel": 3616005058
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback48Header",
   "szText": "フィーランと話す",
   "hLabel": 1698820674
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase52Description",
   "szText": "アルタイルは、カズラ襲撃の解決法について提案がある。だがフィーランはその案を信用しておらず、もっと直接的な方法を望んでいる。カズラの族長を殺して戦利品を持ち帰らせる気だ。",
   "hLabel": 3493812124
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback51Header",
   "szText": "不浄の穴に向かう",
   "hLabel": 917249500
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase55Description",
   "szText": "ブラックホーンを倒してその頭部をフィーランの所に持ちかえらなくては。\r\n\r\n不浄の穴で一族の者を殺せば、奴をおびき出せるはずだ。",
   "hLabel": 1662611391
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback54Header",
   "szText": "血の一族のカズラを倒し、ブラックホーンをおびき出す",
   "hLabel": 496686111
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback65Header",
   "szText": "ブラックホーンを倒し、首を獲る",
   "hLabel": 1456924097
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback60Header",
   "szText": "ブラックホーンを倒し、首を獲る",
   "hLabel": 3589518844
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase63Description",
   "szText": "ブラックホーンを倒し、その首を手に入れた。これをブライクロフト荘園にいるフィーランに届けなくては。",
   "hLabel": 1351728766
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback62Header",
   "szText": "フィーランのもとに戻る",
   "hLabel": 1877487486
  }
 ],
 "ptMapStringTable": "0"
}
