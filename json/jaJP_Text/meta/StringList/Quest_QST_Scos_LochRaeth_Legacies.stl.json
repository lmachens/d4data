 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Name",
   "szText": "開墾",
   "hLabel": 4062401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Toast",
   "szText": "予言者がコーバックで起きている飢饉の根本的な原因を調べるのを手伝う。",
   "hLabel": 141665387
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase0Description",
   "szText": "ベイウィンが族長について言っていたことは正しかった。心底の信仰なのか、あるいは盲目的な服従なのか、どちらにせよ、彼は古の慣習を捨ててコーバックを運命に任せてしまった。\r\n\r\nドメイン・トンネルの近くで予言者に会い、羊頭の獣の襲撃とそれが引き起こした飢饉の真実を探らなければならない。",
   "hLabel": 3577478917
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback5Header",
   "szText": "ドメイン・トンネルの外にいるベイウィンと話す",
   "hLabel": 94135467
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback15Header",
   "szText": "予言者ベイウィンと話す",
   "hLabel": 3113074012
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase16Description",
   "szText": "コーバックの族長が、民に新たな信仰をもたらそうとしてドルイドの結界石を破壊したのは明らかなようだ。\r\n\r\nわかったことについて、ベイウィンと話し合おう。",
   "hLabel": 1452320540
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback64Header",
   "szText": "族長のアイルイグと話す",
   "hLabel": 165456128
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback58Header",
   "szText": "緋色の通路を調べる",
   "hLabel": 1367590691
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase74Description",
   "szText": "ベイウィンと共に飢饉や羊頭の獣の襲撃の原因を探している最中に、ドメイン・トンネルで破壊されたドルイドの結界石を見つけた。\r\n\r\n結界石を破壊した者は石を光の聖堂の聖なる印に置き換えようとしたようだが、印は何の効果もないようだ。トンネルの探索を進めるとしよう。",
   "hLabel": 3504612704
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback80Header",
   "szText": "大聖堂の十字架を調べる",
   "hLabel": 2927058878
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback94Header",
   "szText": "暗黒の降下内を調べる",
   "hLabel": 3466733475
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback105Header",
   "szText": "大聖堂の十字架を調べる",
   "hLabel": 4285028172
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback108Header",
   "szText": "暗黒の降下を調べ続ける",
   "hLabel": 3864464783
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase126Description",
   "szText": "ベイウィンと共にドメイン・トンネルを調べ、飢饉や最近コーバックの周辺で起きている羊頭の獣による襲撃の真相を確かめる必要がある。",
   "hLabel": 1973363342
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback128Header",
   "szText": "ドメイン・トンネルに入る",
   "hLabel": 3202004817
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase65Description",
   "szText": "予言者のベイウィンと共に調査を進めていくと、コーバックの飢饉には、街の族長であるアイルイグと、彼がコーバックを守護している結界石の存在意義を認めていないことが関係しているかもしれないことがわかった。",
   "hLabel": 2994239808
  }
 ],
 "ptMapStringTable": "0"
}
