 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Name",
   "szText": "真菌感",
   "hLabel": 4062401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Toast",
   "szText": "絶望が募る中、より効果的な腹腐れ病の治療薬が求められている。",
   "hLabel": 141665387
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase0Description",
   "szText": "ヨランダから、ウェジンハニの門に釘付けにされた最も新しい狩人の戦利品の頭を取ってくるように頼まれた。妖術師ザークへの捧げ物として相応しいはずだ。",
   "hLabel": 3577478917
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback2Header",
   "szText": "禍々しい勲章の上部を取る",
   "hLabel": 514698856
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase5Description",
   "szText": "妖術師ザークと交信するため、骸泣きの放水地の空き地に行かなければならない。",
   "hLabel": 3388789226
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback4Header",
   "szText": "骸泣きの放水地の空き地を探す",
   "hLabel": 3097634794
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase14Description",
   "szText": "供物を作り、松明を灯した。これから妖術師ザークと話さねばならない。",
   "hLabel": 4104776794
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback13Header",
   "szText": "ザークと話す",
   "hLabel": 530138074
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase19Description",
   "szText": "腹腐れ病の治療法の次の段階について、ヨランダと話をする必要がある。彼女は悪化しているようだ。",
   "hLabel": 3916087103
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback18Header",
   "szText": "ヨランダと話す",
   "hLabel": 2692510623
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase29Description",
   "szText": "ザークから不浄の大蛇ヌロースと呼ばれる蛇を退治するよう依頼された。殺したら、その目を奪ってザークのもとに戻ろう。",
   "hLabel": 952748224
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback53Header",
   "szText": "不浄の大蛇ヌロースを倒して{ITEM}を手に入れる",
   "hLabel": 3500185438
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase32Description",
   "szText": "妖術師ザークと交信するため、骸泣きの放水地の空き地に行かなければ。供物を置いて、近くの松明を灯してみよう。",
   "hLabel": 830555290
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback31Header",
   "szText": "グロテスクな首を供物の器に置く",
   "hLabel": 1579709466
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback63Header",
   "szText": "苔むした松明に火をつける: {DONE_OVER_NEEDED}",
   "hLabel": 3168955455
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase41Description",
   "szText": "妖術師ザークが動物の死体に眼球を入れるように言ってきた。動物が反応した後、治療法を手に入るはずだ。",
   "hLabel": 1340928186
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback40Header",
   "szText": "肥大した蛇の目を置く",
   "hLabel": 4251978810
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase44Description",
   "szText": "ザークからカビの生えた心臓を渡され、腹腐れ病に苦しむ人々を癒すためにウェジンハニに戻るように指示された。治療薬の効果を発揮するまで、ヨランダに十分な時間があることを願う。",
   "hLabel": 3804694749
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback43Header",
   "szText": "モールドの中心にヨランダを連れていく",
   "hLabel": 3831415421
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase49Description",
   "szText": "ザークから不浄の大蛇ヌロースと呼ばれる蛇を退治するよう依頼された。殺したら、その目を奪ってザークのもとに戻ろう。",
   "hLabel": 3616005058
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback48Header",
   "szText": "ザークのもとに戻る",
   "hLabel": 1698820674
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase56Description",
   "szText": "ザークが動物の死体に眼球を入れるように頼まれた。動物が反応した後、治療法が手に入るはずだ。",
   "hLabel": 2483866912
  },
