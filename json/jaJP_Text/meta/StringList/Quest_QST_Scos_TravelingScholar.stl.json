 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Name",
   "szText": "旅する学者",
   "hLabel": 4062401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Toast",
   "szText": "トゥール・ドゥルラまで学者アリアを護衛する。",
   "hLabel": 141665387
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase6Description",
   "szText": "トゥール・ドゥルラにドルイドたちが戻ると知って、旅の学者アリアは大喜びしている。彼らと共に研究をしたいのだ。彼女から、トゥール・ドゥルラまでの護衛を頼まれた。危険に満ちた深き森を通り抜け、無事にたどり着くためだ。",
   "hLabel": 4210044747
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback36Header",
   "szText": "アリアを廃墟へ連れて行く",
   "hLabel": 3742082015
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback86Header",
   "szText": "水辺を調べる",
   "hLabel": 2085932100
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback51Header",
   "szText": "アリアに遺物を見せる",
   "hLabel": 917249500
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase55Description",
   "szText": "古い廃墟でドルイドの秘宝を見つけた。アリアは最後に一度だけ、ある滝に立ち寄りたいそうだ。かつてヴァシリーが瞑想していた滝だ。",
   "hLabel": 1662611391
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback54Header",
   "szText": "滝を見下ろせる場所へとアリアを連れて行く",
   "hLabel": 496686111
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase60Description",
   "szText": "ヴァシリーの滝にいる時、危険な生き物に襲われた。奴らを倒し、アリアを守らなくては。",
   "hLabel": 3182929499
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback59Header",
   "szText": "奇襲をしのぐ",
   "hLabel": 2659058660
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback67Header",
   "szText": "アリアと話す",
   "hLabel": 4039860035
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase69Description",
   "szText": "深き森において最も危険な場所を通り過ぎ、トゥール・ドゥルラの入り口にやってきた。",
   "hLabel": 1984294596
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback71Header",
   "szText": "アリアと話す",
   "hLabel": 254789534
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase76Description",
   "szText": "アリアは、トゥール・ドゥルラに行く前、自分の価値を示すために何か価値のあるものを探していた。",
   "hLabel": 852156450
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback82Header",
   "szText": "ドルイドの遺物を探す{DONE_OVER_NEEDED}",
   "hLabel": 1215027520
  }
 ],
 "ptMapStringTable": "0"
}
