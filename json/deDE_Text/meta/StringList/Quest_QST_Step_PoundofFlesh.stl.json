 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Name",
   "szText": "Ein Pfund Fleisch",
   "hLabel": 4062401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Toast",
   "szText": "Ein Mann in einem Käfig fleht um seine Freiheit.",
   "hLabel": 141665387
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase0Description",
   "szText": "Ich bin auf einen Gefangenen der Banditen gestoßen, der im Onyxturm gefangen gehalten wurde. Er hat mich gebeten, draußen bei den Banditen nach dem Schlüssel zu seinem Käfig zu suchen.",
   "hLabel": 3577478917
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback51Header",
   "szText": "Beschafft den Schlüssel des Käfigs von den Banditen.",
   "hLabel": 917249500
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase10Description",
   "szText": "Ich konnte den Schlüssel zu Nizaars Käfig finden. Ich sollte zum Onyxturm zurückkehren und ihn befreien.",
   "hLabel": 819754710
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback9Header",
   "szText": "Befreit Nizaar aus dem Käfig.",
   "hLabel": 965040047
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase13Description",
   "szText": "Ich habe Nizaar befreit, dem die Zeit im Käfig stark zugesetzt zu haben scheint. Er hat mich gebeten, ihm einen Heiltrank zu geben, um seine Wunden zu heilen.",
   "hLabel": 3283521273
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback12Header",
   "szText": "Gebt Nizaar einen Eurer Heiltränke.",
   "hLabel": 3533637401
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase16Description",
   "szText": "Nizaar und ich gehen jetzt zu Borzas Versteck, um den Mann zu konfrontieren, der ihn eingesperrt hat.",
   "hLabel": 1452320540
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback15Header",
   "szText": "Betretet Borzas Versteck.",
   "hLabel": 3113074012
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase19Description",
   "szText": "Ich habe Nizaar befreit. Aber anscheinend hat ihm die Zeit im Käfig stark zugesetzt.",
   "hLabel": 3916087103
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback18Header",
   "szText": "Sprecht mit Nizaar.",
   "hLabel": 2692510623
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase22Description",
   "szText": "Nizaar will sich an Borza rächen, dem Mann, der ihn in den Käfig gesperrt hatte. Zunächst braucht er jedoch eine Waffe, mit der er kämpfen kann.",
   "hLabel": 3793894169
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback72Header",
   "szText": "Helft Nizaar dabei, eine Waffe auszusuchen.",
   "hLabel": 1546257503
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Phase62Description",
   "szText": "Ich bin auf einen Gefangenen der Banditen gestoßen, der im Onyxturm gefangen gehalten wurde. Er hat mich gebeten, draußen bei den Banditen nach dem Schlüssel zu seinem Käfig zu suchen.",
   "hLabel": 530473245
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Callback64Header",
   "szText": "Findet das verwahrloste Lager.",
   "hLabel": 165456128
  }
 ],
 "ptMapStringTable": "0"
}
